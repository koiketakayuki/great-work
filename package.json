    "lint": "tslint --project .",
    "test": "jest",
    "update-snapshot": "jest --updateSnapshot",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koiketakayuki/great-work.git"
  },
  "author": "",
  "license": "MIT",
